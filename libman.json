{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@7.0.2",
      "destination": "wwwroot/js/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "dayjs@1.11.7",
      "destination": "wwwroot/js/dayjs/",
      "files": [
        "dayjs.min.js",
        "plugin/calendar.js",
        "plugin/customParseFormat.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "bootstrap-table@1.21.2",
      "destination": "wwwroot/lib/bootstrap-table/",
      "files": [
        "dist/bootstrap-table.min.js",
        "dist/bootstrap-table.min.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "qrcodejs@1.0.0",
      "destination": "wwwroot/lib/qrcodejs/"
    }
  ]
}