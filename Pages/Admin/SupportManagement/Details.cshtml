@page
@model Spoonful.Pages.Admin.SupportManagement.DetailsModel
@{
    ViewData["Title"] = "Details";
}
<h1>Details</h1>

<div>
    <h4>Feedback Response: @Html.DisplayFor(model => model.Feedbackform.Id)</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Feedbackform.Customername)
        </dd>
        <dt class="col-sm-2">
            Title
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Feedbackform.TitleFeedback)
        </dd>
        <dt class="col-sm-2">
            Message
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Feedbackform.MainFeedback)
        </dd>
        <dt class="col-sm-2">
            Review
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Feedbackform.Category)
        </dd>
        <dt class="col-sm-2">
            Date Created
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Feedbackform.datetime)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Index">
        <button class="blue-btn">Back to List</button>
        
    </a>
</div>