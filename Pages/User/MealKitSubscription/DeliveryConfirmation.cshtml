<script>
    window.onload = function Function() {
            new SnackBar({
                    message: "Delivery has been confirmed",
                    status: "success",
                    fixed: true,
                    timeout: 10000
                })
            new SnackBar({
                message: "You will be redirected in 5s",
                status: "info",
                fixed: true,
                timeout: 10000
            })
        }

        setTimeout(callBack_func, 5000);
        function callBack_func() {
           document.location.href = "/user/PastInvoices";
        }
</script>